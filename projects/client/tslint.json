{
  "extends": [
    "tslint:latest",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "variable-name": {
      "options": "allow-leading-underscore"
    },
    "object-literal-sort-keys": false,
    "no-console": false,
    "format": "stylish"
  }
}
